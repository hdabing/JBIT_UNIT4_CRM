<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.Customercontact" table="CUSTOMERCONTACT" >
        <id name="id" type="java.lang.Integer">
            <column name="ID" precision="9" scale="0" />
            <generator class="sequence" >
            	<param name="sequence">customercontactid</param>
            </generator>
        </id>
        <property name="name" type="java.lang.String">
        	<column name="NAME" length="20"/>
        </property>
        <property name="gender" type="java.lang.String">
        	<column name="GENDER" length="20"/>
        </property>
        <property name="customerid" type="java.lang.Integer">
        	<column name="CUSTOMERID" />
        </property>
        <property name="position" type="java.lang.String">
        	<column name="POSITION" length="20"/>
        </property>
        <property name="phone" type="java.lang.String">
        	<column name="PHONE" length="20"/>
        </property>
        <property name="telephone" type="java.lang.String">
        	<column name="TELEPHONE" length="20"/>
        </property>
        <property name="content" type="java.lang.String">
        	<column name="CONTENT" length="200"/>
        </property>
        <property name="isdel" type="java.lang.Integer">
        	<column name="ISDEL" />
        </property>
    </class>
</hibernate-mapping>
